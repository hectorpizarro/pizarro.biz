  "scripts": {
    "build": "run-s build:css build:js",
    "build:css": "postcss src/tailwind.source.css -o src/css/index.css --env production",
    "build:js": "react-scripts build",
    "build:style": "tailwind build src/tailwind.source.css -o src/css/index.css -c tailwind.config.js",
    "build:svg": "npx @svgr/cli --icon --replace-attr-values \"#fff=currentColor\" -d src/components/images src/components/images",
    "eject": "react-scripts eject",
    "mockserver": "mockserver -p 8081 -m mockserver",
    "start": "run-p start:express start:css start:js",
    "start:css": "postcss src/tailwind.source.css -o src/css/index.css -w",
    "start:express": "node express-server.js",
    "start:js": "react-scripts start",
    "start:mock": "run-p start:css start:js mockserver",
    "test": "react-scripts test",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
