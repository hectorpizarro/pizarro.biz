    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "axios": "^0.19.0",
    "formik": "^2.0.4",
    "react": "^16.12.0",
    "react-animations": "^1.0.0",
    "react-dom": "^16.12.0",
    "react-modal": "^3.11.1",
    "react-redux": "^7.1.3",
    "react-resize-detector": "^4.2.1",
    "react-router-dom": "^5.1.2",
    "react-scroll": "^1.7.14",
    "react-slick": "^0.25.2",
    "react-slidedown": "^2.4.4",
    "redux": "^4.0.4",
    "redux-act": "^1.7.7",
    "slick-carousel": "^1.8.1",
    "styled-components": "^4.4.1",
    "tailwindcss-transitions": "^2.1.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "purgecss": "^1.4.1",
    "react-scripts": "3.2.0",
    "redux-devtools-extension": "^2.13.8",
    "@svgr/cli": "^4.3.3",
    "tailwindcss": "^1.1.3"
  },
  "scripts": {
    "start": "npm-run-all -p start:css start:js",
    "build": "npm-run-all build:css build:js",
    "start:js": "react-scripts start",
    "build:js": "react-scripts build",
    "start:css": "postcss src/styles/tailwind.source.css -o src/css/index.css -w",
    "build:css": "postcss src/styles/tailwind.source.css -o src/css/index.css --env production",
    "build:style": "tailwind build src/styles/tailwind.source.css -o src/css/index.css -c tailwind.config.js",
    "build:svg": "npx @svgr/cli --icon --replace-attr-values \"#fff=currentColor\" -d src/components/images src/components/images",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
